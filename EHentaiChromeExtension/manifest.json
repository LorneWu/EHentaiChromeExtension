{
  "manifest_version": 2,
  "name": "E-Hentai Helper",
  "description": "Help you download images from E-Hentai and ExHentai.",
  "version": "1.0.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://github.com/neoshell/EHentaiChromeExtension",
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "storage"
  ]
}
